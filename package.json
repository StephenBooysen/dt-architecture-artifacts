{
  "name": "dt-architecture-artifacts",
  "version": "1.0.0",
  "description": "Architecture artifacts markdown editor with glassmorphism theme",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-deps": "npm install && cd client && npm install",
    "test": "jest --config tests/jest.config.js",
    "test:watch": "jest --config tests/jest.config.js --watch",
    "test:coverage": "jest --config tests/jest.config.js --coverage",
    "test:client": "cd client && npm test",
    "test:server": "jest --config tests/jest.config.js --testPathPattern=server"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "*",
    "cors": "*",
    "dotenv": "*",
    "express": "*",

    "express-rate-limit": "*",
    "helmet": "*",
    "jsonwebtoken": "*",
    "multer": "^2.0.1",
    "simple-git": "*"
  },
  "devDependencies": {
    "concurrently": "*",
    "nodemon": "*",
    "@babel/core": "^7.22.0",
    "@babel/preset-env": "^7.22.0",
    "@babel/preset-react": "^7.22.0",
    "@babel/plugin-proposal-class-properties": "^7.18.0",
    "@babel/plugin-transform-runtime": "^7.22.0",
    "babel-jest": "^29.5.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "identity-obj-proxy": "^3.0.0",
    "supertest": "^6.3.3"
  }
}
