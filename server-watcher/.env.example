# Architecture Artifacts Server Watcher Environment Configuration
# Copy this file to .env and fill in your actual values

# Server Configuration
WATCHER_SERVER_URL=http://localhost:3001

# Authentication
WATCHER_API_KEY=ak_your_generated_api_key_here
WATCHER_USERNAME=your_username

# Paths
WATCHER_LOCAL_PATH=./sync-folder
WATCHER_REMOTE_PATH=synced-files

# Options
WATCHER_VERBOSE=false
WATCHER_DRY_RUN=false